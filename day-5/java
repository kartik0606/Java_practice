public class Main {
    public static void main(String[] args) {
        int n = 32456;
        int rotate = 0;
        int k = 2;
        int temp = n;
        int count = 0;

        while ( temp > 0 ) {
             temp = temp / 10;
             count++;
        }
          temp = n ;
         int power = (int) Math.pow(10,count-1);
          int index = 0 ;

          while (power > 0 ){
              int digit  = temp /power;
              temp  = temp % power;

              int newindex = (index + k)%count;
               if (newindex < 0 ){
                   newindex = newindex + count;
               }
                 rotate = rotate + digit*(int)Math.pow(10,count-1-newindex);
                  power = power/10;
                  index++;
          }
        System.out.println(rotate);

    }
}


import java.util.Scanner;

public class Main {
     public static void main(String[] args) {
         Scanner input = new Scanner(System.in);
         int n  = 3214;
         int inverse  = 0 ;
         int position = 1;
         while (n > 0 ){
               int digit = n % 10;
                   n = n/10;
                inverse = inverse + position *(int)Math.pow(10,digit-1);
                position++;
         }
         System.out.println(inverse);
     }
}

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int n = 240;
        for (int div = 2; div<=n; div++) {

            while (n % div == 0 ){
                n = n /div;
                System.out.println(div);
            }

        }

    }

  public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("write for h ");
        int h = input.nextInt();
        System.out.println("write for p");
        int p = input.nextInt();
        System.out.println("write for b");
        int b = input.nextInt();

       if(h*h == p*p + b*b){
           System.out.println(true);

        }else {
           System.out.println(false);
       }
    }
}

 public class Main {
     public static void main(String[] args) {
         int n =44;
         for (int row = 1; row <=n ; row++) {

             for (int col = 1; col <=row ; col++) {
                 System.out.print(" * ");
             }
             System.out.println();

         }
     }
}
