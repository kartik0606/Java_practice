  public class Main {
      public static void main(String[] args) {
          int n = 1651;
          int reverse = 0;
          int count = 0;
          int original = n;
          while (n > 0) {
              int n1 = n % 10;
              reverse = reverse * 10 + n1;
              count++;
              n = n / 10;

          }
           System.out.println(reverse);

          if (reverse == original) {
              System.out.println("palindrome");
          }else {
              System.out.println("not palindrome");
          }

      }
  }
public class Main {
    public static void main(String[] args) {
        int n = 153;
        int original = n;
        int arms  = 0 ;
        int count=0;
        while (n > 0 ){
             int n1 = n% 10;
            arms = arms + (n1 * n1 * n1);
            count++;
             n = n/10;

        }
        System.out.println(arms);

         if (arms ==original){
             System.out.println("arms");

         }else {
             System.out.println("no");
         }

    }
}
 public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int target  = 42;
        int guess = 0;
        while (guess!=target){
             guess = input.nextInt();;
            System.out.println("enter again;");
             if (guess > target){
                 System.out.println("too high");
             } else if (guess<target) {
                 System.out.println("too low");

             }else {
                 System.out.println("congratulations u win this game ");
             }

        }
    }
}
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
