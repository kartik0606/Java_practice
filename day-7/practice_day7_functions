 public class Main {
    static int x =90;
     public static void main(String[] args) {
         System.out.println(x);
         int x =40;
         System.out.println(x);
         fun();
     }
     static void fun(){
         System.out.println(x);//this x is 90 because it is the first x which have a maximum scope
   }
}
  public class Main {
    public static void main(String[] args) {
      fun(67);
      fun("kartik",3);

    }
     static void fun(int a ){
       System.out.println(a);
     }
     static void fun(String name,int b){
       System.out.println(name+ " " + b);
     }
}
